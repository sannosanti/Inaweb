  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title text-center"  >useros</h3>
    </div>
    <table class="table table-hover">
    <thead>
      <tr>
        <th>Titulo</th>
        <th>Categoria</th>
        <th>Descripcion</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.email %></td>
          <td class= "text-center">
            <%= link_to admin_user_path(user), class:'btn btn-default btn-sm' do %>
              <i class="fa fa-eye fa-2x"></i>
            <% end %>
          </td>         
          <td class= "text-center">
          <%= link_to edit_admin_user_path(user), class: 'btn btn-default btn-sm'  do %>     
            <i class="fa fa-pencil fa-2x" ></i>
          </td>
          <% end %>

          <td class= "text-center"><%= link_to admin_user_path(user), class:'btn btn-default btn-sm', method: :delete, data: { confirm: 'Are you sure?' } do %>
              <i class="fa fa-trash fa-2x"></i>
            <% end %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>

  </div>

  <br>
<div class= "text-center"><%= link_to 'Nuevo usuario', new_admin_user_path, class:'btn btn-default' %></div>
  